        "snapshot-diff": "^0.5.2",
        "typescript": "^3.5.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "cd ../../ && jest test  clients/core --verbose --collectCoverage=true && cd clients/core",
        "test:watch": "cd ../../ && jest test  clients/core --verbose --watch && cd clients/core",
        "eject": "react-scripts eject",
        "lint": "eslint './**/*.{js,jsx,ts,tsx}'"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "git add"
        ],
        "*.{js,jsx}": [
            "eslint"
        ],
        "*.{json,css,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{ts,tsx, js, jsx}",
            "!**/node_modules/**",
            "!**/coverage/**",
            "!**/serviceWorker.ts",
            "!**/setupTests.ts",
            "!src/index.tsx",
            "!**/*.d.ts"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 80,
                "functions": 90,
                "lines": 90,
                "statements": 90
            }
        }
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@types/react-helmet": "^5.0.15",
        "@types/react-select": "^3.0.10"
    }
}
