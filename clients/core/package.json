        "@types/react-router": "^5.0.3",
        "@types/react-router-dom": "^4.3.4",
        "@types/react-select": "^3.0.10",
        "@types/reactstrap": "^8.0.1",
        "@types/seamless-immutable": "^7.1.10",
        "bootstrap": "^4.3.1",
        "classnames": "^2.2.6",
        "client-oauth2": "^4.2.5",
        "connected-react-router": "^6.5.2",
        "fetch-mock": "^7.3.9",
        "formik": "^2.1.4",
        "history": "^4.9.0",
        "jest-fetch-mock": "^2.1.2",
        "lodash": "^4.17.15",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-helmet": "^5.2.1",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "react-scripts": "^3.0.1",
        "react-select": "^3.0.8",
        "reactstrap": "^8.0.1",
        "redux": "^4.0.4",
        "redux-thunk": "^2.3.0",
