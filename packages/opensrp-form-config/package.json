{
    "name": "@opensrp/form-config",
    "version": "0.0.4",
    "description": "OpenSrp form configuration components",
    "main": "dist/index.js",
    "types": "dist/types",
    "files": [
        "dist"
    ],
    "publishConfig": {
        "access": "public"
    },
    "repository": "https://github.com/opensrp/opensrp-web",
    "scripts": {
        "jest": "jest --coverage --verbose --color",
        "test": "cd ../../ && jest test  packages/opensrp-server-service --verbose --collectCoverage=true && cd packages/opensrp-server-service",
        "test:watch": "cd ../../ && jest test  packages/opensrp-server-service --verbose --watch && cd packages/opensrp-server-service",
        "tsc": "tsc",
        "lint": "eslint './**/*.{js,jsx,ts,tsx}'",
        "transpile": "babel src -d dist --root-mode upward --extensions '.ts,.tsx' --ignore '**/*.test.ts,**/*.test.tsx,**/tests,**/__tests__'"
    },
    "jest": {
        "automock": false,
        "setupFiles": [
            "../../setupTests"
        ]
    },
    "peerDependencies": {
        "react": "^16.8.6",
        "react-redux": "^7.1.0",
        "react-router": "^5.0.1",
        "react-router-dom": "^5.0.1",
        "redux": "^4.0.4"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.20",
        "@fortawesome/free-brands-svg-icons": "^5.10.0",
        "@fortawesome/free-regular-svg-icons": "^5.10.0",
        "@fortawesome/free-solid-svg-icons": "^5.10.0",
        "@fortawesome/react-fontawesome": "^0.1.4",
        "@onaio/drill-down-table": "^1.0.2",
        "@onaio/redux-reducer-registry": "^0.0.9",
        "@opensrp/reducer-factory": "^0.0.1",
        "@opensrp/server-service": "^0.0.2",
        "formik": "^2.1.4",
        "lodash": "^4.17.15",
        "react-toastify": "^6.0.5",
        "reactstrap": "^8.0.1",
        "yup": "^0.29.1"
    },
    "bugs": {
        "url": "https://github.com/opensrp/opensrp-web/issues"
    },
    "author": "Ona Engineering",
    "license": "Apache-2.0",
    "private": false
}
